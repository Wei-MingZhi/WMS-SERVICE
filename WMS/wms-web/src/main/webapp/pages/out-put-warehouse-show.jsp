            <h1>
                物品出入库申请 <small>物品申请</small>
            </h1>
            <ol class="breadcrumb">
                <li><a href="main.jsp"><i class="fa fa-dashboard"></i> 首页</a></li>
                <li><a href="${pageContext.request.contextPath}/out-put-warehouse/material-list.do">物品出入库申请</a></li>
                <li class="active">物品申请</li>
            </ol>
        </section>
        <!-- 内容头部 /-->

        <form action="${pageContext.request.contextPath}/out-put-warehouse/add-warehouse-record.do" method="post">
            <!-- 正文区域 -->
            <section class="content">
                <!--物品信息-->
                <div class="panel panel-default">
                    <div class="panel-heading">物品出入库申请</div>
                    <div class="row data-type">
                        <input type="text" hidden="hidden" class="form-control" name="materialsId" value="${materials.id}">
                        <div class="col-md-2 title">物品名称</div>
                        <div class="col-md-4 data">
                            <input type="text"readonly class="form-control" name="materialsName" placeholder="物品名称" value="${materials.materialsName}">
                        </div>
                        <div class="col-md-2 title">物品编号</div>
                        <div class="col-md-4 data">
                            <input type="text" readonly class="form-control" name="materialsNumber" placeholder="物品编号" value="${materials.materialsNumber}">
                        </div>
                        <div class="col-md-2 title">库存数量</div>
                        <div class="col-md-4 data">
                            <input type="text" readonly class="form-control" name="inventoryNumber" placeholder="库存数量" value="${materials.inventoryNumber}">
                        </div>
                        <div class="col-md-2 title"></div>
                        <div class="col-md-4 data">
                            <input disabled="disabled" type="text" class="form-control" name="" placeholder="" value="">
                        </div>
                        <div class="col-md-2 title">申请数量</div>
                        <div class="col-md-4 data">
                            <input type="number" class="form-control" name="applyNumber" placeholder="申请数量" value="">
                        </div>
                        <div class="col-md-2 title">申请动作</div>
                        <div class="col-md-4 data">
                            <select class="form-control select2" style="width: 100%" name="applyStatus">
                                <option value="0" selected="selected">出库</option>
                                <option value="1">入库</option>
                            </select>
                        </div>
                        <div class="col-md-2 title rowHeight2x">备注</div>
                        <div class="col-md-10 data rowHeight2x">
                            <textarea class="form-control" rows="3" placeholder="备注" name="remark" ></textarea>
                        </div>

                    </div>
                </div>
                <!--工具栏-->
                <div class="box-tools text-center">
                    <button type="submit" class="btn bg-maroon">申请</button>
                    <a type="button" class="btn bg-default" style="background-color: #949494;color: #ffffff" href="${pageContext.request.contextPath}/out-put-warehouse/material-list.do" >返回</a>
                </div>
                <!--工具栏/-->
            </section>
            <!-- 正文区域 /-->
        </form>
